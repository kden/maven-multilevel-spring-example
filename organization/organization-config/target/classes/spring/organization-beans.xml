<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
        http://www.springframework.org/schema/util
        http://www.springframework.org/schema/util/spring-util-4.3.xsd">

    <bean id="owner" class="org.example.Person"
    >
        <property name="firstName" value="Jane" />
        <property name="lastName" value="Doe" />
    </bean>

    <bean id="someguy" name="someguy" class="org.example.Person"
    >
        <property name="firstName" value="Wrigley" />
        <property name="lastName" value="Jackson" />
    </bean>
</beans>
